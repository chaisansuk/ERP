<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="5dp"
    android:layout_marginBottom="10dp"
    android:background="#ffffff"
    android:weightSum="1">


    <android.support.constraint.ConstraintLayout
        android:layout_width="401dp"
        android:layout_height="114dp"
        android:orientation="horizontal"
        android:layout_marginLeft="3dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="3dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="3dp"
        android:layout_marginEnd="3dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.006">

        <ImageView
            android:id="@+id/img_type"
            android:layout_width="87dp"
            android:layout_height="82dp"

            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="29dp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/date"
            app:layout_constraintVertical_bias="0.0"
            android:layout_marginLeft="278dp"
            app:layout_constraintLeft_toLeftOf="parent" />

        <TextView
            android:id="@+id/project_name"
            android:layout_width="331dp"
            android:layout_height="23dp"
            android:layout_marginRight="13dp"
            android:layout_marginTop="6dp"
            android:text="project item"
            android:textAlignment="viewStart"
            android:textSize="15dp"
            android:textStyle="bold"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="0.529"
            tools:layout_editor_absoluteY="7dp"
            tools:layout_editor_absoluteX="60dp" />

        <TextView
            android:id="@+id/date"
            android:layout_width="274dp"
            android:layout_height="23dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="7dp"
            android:layout_marginTop="103dp"
            android:text="20-65-78"
            android:textAlignment="textStart"
            android:textSize="15dp"
            app:layout_constraintHorizontal_bias="0.232"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintVertical_bias="0.75" />

        <TextView
            android:id="@+id/run_number"
            android:layout_width="330dp"
            android:layout_height="26dp"
            android:text="987678765568"
            android:textSize="15dp"
            android:textStyle="italic"
            app:layout_constraintLeft_toLeftOf="@+id/project_name"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="1dp"
            app:layout_constraintTop_toBottomOf="@+id/project_name"
            app:layout_constraintHorizontal_bias="0.0"
            tools:layout_editor_absoluteY="28dp"
            tools:layout_editor_absoluteX="88dp" />

        <TextView
            android:id="@+id/create_user"
            android:layout_width="331dp"
            android:layout_height="25dp"
            android:text="admin"
            android:textSize="15dp"
            android:textStyle="italic"
            app:layout_constraintRight_toRightOf="@+id/project_name"
            android:layout_marginRight="148dp"
            android:layout_marginTop="3dp"
            app:layout_constraintTop_toBottomOf="@+id/run_number"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="@+id/run_number"
            app:layout_constraintHorizontal_bias="0.049"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/date"
            app:layout_constraintVertical_bias="0.0"
            tools:layout_editor_absoluteY="59dp"
            tools:layout_editor_absoluteX="80dp" />
    </android.support.constraint.ConstraintLayout>


</android.support.constraint.ConstraintLayout>

