<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="5dp"
    android:layout_marginBottom="10dp"
    android:background="#ffffff"
    android:weightSum="1">


    <android.support.constraint.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="85dp"
        android:orientation="horizontal"
        android:layout_marginLeft="3dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="3dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="3dp"
        android:layout_marginEnd="3dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/next"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.492"
            app:srcCompat="@drawable/next"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/date"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" />

        <ImageView
            android:id="@+id/img_type"
            android:layout_width="57dp"
            android:layout_height="48dp"

            android:layout_marginLeft="7dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="26dp" />

        <TextView
            android:id="@+id/project_name"
            android:layout_width="wrap_content"
            android:layout_height="23dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="13dp"
            android:layout_marginTop="9dp"
            android:text="project item"
            android:textAlignment="viewStart"
            android:textSize="15dp"
            android:textStyle="bold"
            app:layout_constraintHorizontal_bias="0.141"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="13dp" />

        <TextView
            android:id="@+id/date"
            android:layout_width="260dp"
            android:layout_height="23dp"
            android:layout_marginRight="6dp"
            android:text="20-65-78"
            android:textAlignment="textStart"
            android:textSize="15dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="35dp"
            app:layout_constraintTop_toBottomOf="@+id/run_number"
            app:layout_constraintVertical_bias="0.193"
            app:layout_constraintLeft_toRightOf="@+id/img_type"
            android:layout_marginLeft="8dp"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="6dp"
            tools:layout_editor_absoluteY="51dp"
            tools:layout_editor_absoluteX="74dp" />

        <TextView
            android:id="@+id/run_number"
            android:layout_width="134dp"
            android:layout_height="24dp"
            android:text="0000-0-000-00000"
            android:textSize="15dp"
            android:textStyle="italic"
            android:layout_marginTop="1dp"
            app:layout_constraintTop_toBottomOf="@+id/project_name"
            app:layout_constraintLeft_toRightOf="@+id/img_type"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" />

        <TextView
            android:id="@+id/create_user"
            android:layout_width="144dp"
            android:layout_height="24dp"
            android:text="admin"
            android:textSize="15dp"
            android:textStyle="italic"
            app:layout_constraintRight_toRightOf="@+id/project_name"
            android:layout_marginRight="22dp"
            app:layout_constraintTop_toBottomOf="@+id/project_name"
            app:layout_constraintLeft_toRightOf="@+id/run_number"
            app:layout_constraintHorizontal_bias="0.004"
            android:layout_marginEnd="22dp" />
    </android.support.constraint.ConstraintLayout>


</android.support.constraint.ConstraintLayout>

